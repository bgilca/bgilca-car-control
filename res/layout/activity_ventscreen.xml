<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background1"
    android:keepScreenOn="true"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.corrado.bgilcag60.ventscreen" >

    <Button
        android:id="@+id/scaunstg"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/scaunstg"
        android:baselineAlignBottom="true"
        android:clickable="true" />

    <Button
        android:id="@+id/scaundr"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/scaundr"
        android:baselineAlignBottom="true"
        android:clickable="true" />

    <Button
        android:id="@+id/home"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="5dp"
        android:background="@drawable/corrado_home"
        android:clickable="true" />

    <Button
        android:id="@+id/head"
        android:layout_width="40dp"
        android:layout_height="53dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="5dp"
        android:background="@drawable/head_tab"
        android:clickable="true" />

    <Button
        android:id="@+id/navi"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginRight="40dp"
        android:layout_marginTop="10dp"
        android:layout_toLeftOf="@+id/head"
        android:background="@drawable/navi_tab"
        android:clickable="true" />

    <Button
        android:id="@+id/fantab"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginRight="50dp"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/navi"
        android:background="@drawable/fan_tab_fan"
        android:clickable="true" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_below="@+id/head"
        android:layout_marginTop="60dp"
        android:layout_toLeftOf="@+id/head"
        android:src="@drawable/cap" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:layout_below="@+id/imageView4"
        android:layout_marginRight="10dp"
        android:layout_toLeftOf="@+id/head"
        android:src="@drawable/corp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="127dp"
        android:layout_height="80dp"
        android:layout_below="@+id/imageView4"
        android:layout_marginRight="30dp"
        android:layout_marginTop="56dp"
        android:layout_toLeftOf="@+id/head"
        android:src="@drawable/picioare"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/ac"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_centerVertical="true"
        android:background="@drawable/clima_monozon" />

    <ImageView
        android:id="@+id/deg"
        android:layout_width="30dp"
        android:layout_height="15dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ac"
        android:layout_marginRight="130dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/celsius" />

    <ImageView
        android:id="@+id/degwan"
        android:layout_width="45dp"
        android:layout_height="15dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ac"
        android:layout_marginRight="160dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/gol" />

    <Button
        android:id="@+id/tempplus"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/ac"
        android:layout_centerHorizontal="true"
        android:background="@drawable/gol" />

    <Button
        android:id="@+id/tempminus"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/ac"
        android:layout_centerHorizontal="true"
        android:background="@drawable/gol" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:background="@drawable/fan" />

    <ImageView
        android:id="@+id/fanspeed"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="130dp"
        android:background="@drawable/gol" />

    <Button
        android:id="@+id/fanplus"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignTop="@+id/imageView1"
        android:background="@drawable/gol" />

    <Button
        android:id="@+id/fanminus"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/imageView1"
        android:layout_alignLeft="@+id/imageView1"
        android:background="@drawable/gol" />

    <Button
        android:id="@+id/cap"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_below="@+id/head"
        android:layout_marginRight="50dp"
        android:layout_marginTop="70dp"
        android:layout_toLeftOf="@+id/head"
        android:background="@drawable/capatana" />

    <Button
        android:id="@+id/corp"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_above="@+id/craci"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="25dp"
        android:layout_marginLeft="80dp"
        android:background="@drawable/corpi" />

    <Button
        android:id="@+id/craci"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_above="@+id/ac"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="30dp"
        android:background="@drawable/crac" />

</RelativeLayout>